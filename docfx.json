{
  "metadata": [
    {
      "src": [
        {
          "files": [ "src/**/*.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ]
        }
      ],
      "dest": "docs/api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "EnumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "articles/**/*.md", "articles/**/toc.yml" ],
        "src": "docs",
        "dest": "."
      },
      {
        "files": [ "api/**/*.yml", "api/index.md" ],
        "src": "docs",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "src": "docs",
        "dest": "."
      }
    ],
    "output": "docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default", "modern" ],
    "postProcessors": [],
    "globalMetadata": {
      "_appTitle": "DocumentGenerator",
      "_appName": "DocumentGenerator",
      "_appFooter": "DocumentGenerator â€” Badge &amp; Document Rendering Service",
      "_enableSearch": true,
      "_appLogoPath": "",
      "_disableContribution": true
    },
    "sitemap": {
      "baseUrl": "http://localhost:8081"
    }
  }
}
